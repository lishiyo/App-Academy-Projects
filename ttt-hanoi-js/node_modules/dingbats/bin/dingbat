#!/usr/bin/env node

var dingbats = require('../lib/dingbats');

var args = process.argv.slice(2).join(' ').split('.');

if (process.argv.length < 3
|| process.argv.some(function (a) { return a == '--help'; })) {
  console.log(dingbats);
}
else {
  var char = dingbats;

  args.forEach(function (n) {
    char = char[n];
  });

  console.log(char);
}
