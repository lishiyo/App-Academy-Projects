
<div class="row">
	<h1>Contact</h1>
	<div class="col-lg-6 col-md-6 col-xs-6">
	<form>
		<input
			type="hidden"
			name="authenticity_token"
			value="<%= form_authenticity_token %>">
		
		<div class="form-group">
			<label for="user_username">Email</label>
			<input class="form-control" type="text" name="user[email]" id="user_email">
		</div>
		<div class="form-group">
			<label for="user_password">Message</label>
			<input class="form-control" type="textarea" name="user[text]" id="user_email">
		</div>

		<br>
		<input type="button" value="Log in!" class="form-control btn btn-primary">
	</form>
	</div>
	
	<div class="col-lg-6 col-md-6 col-xs-6">
		<table class="table table-striped table-hover">
  		<tr colspan="2">
				<th>Staff Names</th>
				<th>Phone Numbers</th>
			</tr>
			<tr>
				<td>Sennacy</td>
				<td>100-0000</td>
			</tr>
			<tr>
				<td>Rocky</td>
				<td>100-1000</td>
			</tr>
		</table>
	</div>
	
</div>